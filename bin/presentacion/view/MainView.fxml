<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" stylesheets="@black.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.control.MainUIController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="menuClose" mnemonicParsing="false" onAction="#exit" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Customer">
            <items>
                  <MenuItem fx:id="menuCrearCliente" mnemonicParsing="false" onAction="#crearCliente" text="CrearCliente" />
                  <MenuItem fx:id="menuListarClientes" mnemonicParsing="false" onAction="#listarClientes" text="ListarClientes" />
                  <MenuItem fx:id="menuCrearReserva" mnemonicParsing="false" onAction="#crearReserva" text="CrearReserva" />
            </items>
          </Menu>
          
          <Menu mnemonicParsing="false" text="Admin">
               <items>
                  <MenuItem fx:id="menuCrearMonitor" mnemonicParsing="false" onAction="#crearMonitor" text="Crear Monitor" />
                  <MenuItem fx:id="menuCrearClase" mnemonicParsing="false" onAction="#crearClase" text="Crear Clase" />
               </items></Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
